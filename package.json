{
  "name": "remark-validate-vfile",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "description": "remark plugin for validating the state of a VFile against a set of user-defined assertions.",
  "exports": "./index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "env": {
      "es2022": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "unicorn",
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:unicorn/recommended"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alecvision/remark-validate-vfile.git"
  },
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "vfile",
    "validate",
    "validation"
  ],
  "author": "Alec Helmturner <git@alecvision.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alecvision/remark-validate-vfile/issues"
  },
  "homepage": "https://github.com/alecvision/remark-validate-vfile#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-plugin-unicorn": "^43.0.0",
    "unified": "^10.1.2",
    "vfile": "^5.3.4"

  }
}
